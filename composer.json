{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for GT Drupal 8 projects with Composer",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "IC Webteam",
      "email": "webteam@gatech.edu",
      "role": "Makers of things like this"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.gatech.edu/ICWebTeam/gt_profile_curie.git"
    },
    {
      "type": "vcs",
      "url": "https://github.gatech.edu/ICWebTeam/gt_theme-8.x.git"
    },
    {
      "type": "vcs",
      "url": "https://github.gatech.edu/ICWebTeam/gt_tools-8.x.git"
    },
    {
      "type": "vcs",
      "url": "https://github.gatech.edu/ICWebTeam/hg_reader-8.x.git"
    }
  ],
  "require": {
    "php": ">=7.3",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/admin_toolbar": "^2.4",
    "drupal/cas": "^1.7",
    "drupal/core-composer-scaffold": "^8.8",
    "drupal/core-project-message": "^8.8",
    "drupal/core-recommended": "^8.9",
    "drupal/field_formatter_class": "^1.4",
    "drupal/field_group": "^3.1",
    "drupal/layout_builder_modal": "^1.1",
    "drupal/layout_builder_restrictions": "^2.7",
    "drupal/migrate_plus": "^5.1",
    "drupal/migrate_tools": "^5.0",
    "drupal/migrate_upgrade": "^3.2",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/social_media_links": "^2.7",
    "drupal/video_embed_field": "^2.4",
    "drush/drush": "^10.0",
    "gt/gt_profile_curie": "^1.6",
    "gt/gt_theme": "^1.3",
    "gt/gt_tools": "^1.0",
    "gt/hg_reader": "^1.6",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3"
  },
  "conflict": {
      "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "sort-packages": true
  },
  "extra": {
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/{$name}": ["type:drupal-drush"]
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "web"
      },
      "file-mapping": {
        "[web-root]/.htaccess": {
          "mode": "replace",
          "path": "assets/htaccess",
          "overwrite": false
        }
      }
    }
  }
}
